label.ss.heavy= strings.claims.removeYoutubeClaims
p.ss.cl-error(role='error').hide= strings.claims.error
p.ss.cl-info(role='info').hide
table.ss.rows
  tbody
    tr
      td
        i(role="loading").fa.fa-refresh.fa-spin.hide
      td
        label.ss=strings.claims.link
      td
        input.ss(type="text", role="youtube-link")
      td
        button.ss(role='remove-channel')=strings.claims.remove
